{
  "name": "nonote",
  "version": "1.3.1",
  "description": "a node application for taking notes in markdown from the command line",
  "main": "./lib/main.js",
  "scripts": {
    "build": "./node_modules/babel-cli/bin/babel.js src --out-dir lib --source-maps",
    "watch": "babel src --out-dir lib --source-maps --watch",
    "start": "node lib/main.js"
  },
  "babel": {
    "presets": [
      "es2015-node5"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michaelghinrichs/nonote.git"
  },
  "keywords": [
    "node",
    "notes",
    "cli"
  ],
  "author": "Michael Hinrichs",
  "license": "ISC",
  "bin": {
    "nonote": "./lib/main.js",
    "nono": "./lib/main.js"
  },
  "bugs": {
    "url": "https://github.com/michaelghinrichs/nonote/issues"
  },
  "homepage": "https://github.com/michaelghinrichs/nonote#readme",
  "dependencies": {
    "babel-polyfill": "^6.7.4",
    "chalk": "^1.1.1",
    "co": "^4.6.0",
    "co-prompt": "^1.0.0",
    "commander": "^2.9.0",
    "fs-extra": "^0.26.7",
    "jsonfile": "^2.2.3",
    "moment": "^2.12.0",
    "source-map-support": "^0.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015-node5": "^1.2.0",
    "json-loader": "^0.5.4"
  }
}
